{
  "eventsAndFunctions": [
    {
      "algorithm": {
        "parameters": [],
        "statements": [
          {
            "command": "Set",
            "expression": "{}",
            "variableName": "myPet"
          },
          {
            "command": "GetValue",
            "variableName": "myPhotoUrl",
            "widgetName": "textbox1"
          },
          {
            "children": [
              {
                "children": [
                  {
                    "command": "Set",
                    "expression": "[]",
                    "variableName": "myPet.photoUrls"
                  }
                ],
                "command": "If",
                "expression": "IsNull(^myPet.photoUrls)"
              },
              {
                "command": "Push",
                "expressionItem": "^myPhotoUrl",
                "variableNameList": "myPet.photoUrls"
              },
              {
                "command": "SendDataToParent",
                "expressionData": "{ petWithPhoto: ^myPet }"
              },
              {
                "command": "WriteLocalStorage",
                "expression": "^myPet",
                "localStorageVariableName": "instance"
              },
              {
                "command": "Execute",
                "expression": "PetStore.UpdatePet(^myPet)"
              }
            ],
            "command": "If",
            "expression": "IsNotNull(^myPhotoUrl)"
          },
          {
            "children": [
              {
                "command": "AlertMessage",
                "expressionMessage": "\"Please insert a photo URL to add\"",
                "expressionTitle": "\"No Photo Url\"",
                "severity": "warning"
              }
            ],
            "command": "Else"
          }
        ],
        "typeReturn": "any",
        "variables": [
          {
            "name": "myPhotoUrl",
            "type": "string"
          },
          {
            "name": "myPet",
            "type": "PetStore.Pet"
          }
        ]
      },
      "name": "button1_click"
    }
  ],
  "model": [
    {
      "name": "instance",
      "required": false,
      "type": "PetStore.Pet"
    }
  ],
  "root": {
    "children": [
      {
        "label": "[[PhotoUrl]]",
        "maxLength": 1000,
        "name": "textbox1",
        "widget": "TextBox"
      },
      {
        "name": "button1",
        "widget": "Button"
      }
    ],
    "cssClass": "blz-area-default",
    "layout": "container"
  },
  "routing": null,
  "unused": [
    {
      "__internalName": "label1",
      "color": "default",
      "name": "label1",
      "text": "Photo is added here via url",
      "widget": "Label"
    }
  ]
}