{
  "eventsAndFunctions": [
    {
      "algorithm": {
        "parameters": [],
        "statements": [
          {
            "command": "Set",
            "expression": "{}",
            "variableName": "myPet"
          },
          {
            "command": "GetValue",
            "variableName": "myPet.name",
            "widgetName": "txtName"
          },
          {
            "command": "GetValue",
            "variableName": "myPet.status",
            "widgetName": "txtStatus"
          },
          {
            "command": "ConsoleLog",
            "expression": "^myPet"
          },
          {
