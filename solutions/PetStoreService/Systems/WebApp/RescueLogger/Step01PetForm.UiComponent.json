            "severity": "information"
          },
          {
            "command": "WriteLocalStorage",
            "expression": "^myPet",
            "localStorageVariableName": "\"instance\""
          },
          {
            "command": "SendDataToParent",
            "expressionData": "{ newpet: ^myPet }"
          }
        ],
        "typeReturn": "any",
        "variables": [
          {
            "name": "myPet",
            "type": "PetStore.Pet"
          }
        ]
      },
      "name": "button1_click"
    }
  ],
  "model": [
    {
      "name": "instance",
      "required": false,
      "type": "PetStore.Pet"
    }
  ],
  "root": {
    "children": [
      {
        "children": [
          {
            "binding": "instance.name",
            "label": "[[Name]]",
            "name": "txtName",
            "widget": "TextBox"
          },
          {
            "binding": "instance.status",
            "label": "[[Status]]",
            "name": "txtStatus",
            "widget": "TextBox"
          },
          {
            "align": "right",
