{
  "algorithm": {
    "parameters": [],
    "statements": [
      {
        "command": "ReadConfigParameter",
        "configParameterName": "DataSource",
        "variableName": "source"
      },
      {
        "children": [
          {
            "command": "Return",
            "expression": "Transform(TrfPetFromRest, PetStore.FindPetsByStatus({status: \"available\"}))"
          }
        ],
        "command": "If",
        "expression": "^source == \"rest\""
      },
      {
        "children": [
          {
            "command": "Return",
            "expression": "Transform(TrfPetFromDb, RdsSelectReader(Mydb.Pets))"
          }
        ],
        "command": "Else"
      }
    ],
    "typeReturn": "list(PetMine)",
    "variables": [
      {
        "name": "source",
        "type": "string"
      }
    ]
  },
  "routing": {
    "method": "GET",
    "path": "/getpetfromsource"
  }
}