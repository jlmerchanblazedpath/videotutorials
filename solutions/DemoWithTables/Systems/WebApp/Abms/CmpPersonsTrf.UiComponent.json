            "mode": "right-drawer",
            "size": "default",
            "uiComponentName": "CmpPersonsTrfForm"
          },
          {
            "children": [
              {
                "command": "Set",
                "expression": "^openedModel.instance",
                "variableName": "myPerson"
              },
              {
                "command": "Execute",
                "expression": "MyMicroservice.UpdatePerson(^myPerson.id, ^myPerson)"
              },
              {
                "command": "Refresh",
                "widgetName": "grid1"
              },
              {
                "command": "CloseCurrent"
              }
