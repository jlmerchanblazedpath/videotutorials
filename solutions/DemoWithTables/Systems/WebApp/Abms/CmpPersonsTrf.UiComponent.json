            "command": "Set",
            "expression": "{}",
            "variableName": "args1"
          },
          {
            "command": "Set",
            "expression": "Map(FindFirst(^args.filters, key == \"f2\"), value)",
            "variableName": "args1.genderId"
          },
          {
            "command": "ConsoleLog",
            "expression": "^args"
          },
          {
            "command": "ConsoleLog",
            "expression": "args1.genderId"
          },
          {
            "command": "ConsoleLog",
            "expression": "^args1.genderId"
          },
          {
